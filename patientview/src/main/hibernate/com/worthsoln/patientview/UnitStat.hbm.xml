<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.unitstat.UnitStat" table="unitstat">
    <id name="id"
        unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="unitcode"/>
    <property name="yearmonth"/>
    <property name="action"/>
    <property name="count"/>
  </class>
</hibernate-mapping>