<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.User" table="user">
    <id name="username">
      <generator class="assigned"/>
    </id>
    <property name="password"/>
    <property name="role"/>
    <property name="name"/>
    <property name="email"/>
    <property name="emailverified"/>
    <property name="firstlogon"/>
    <property name="lastlogon"/>
    <property name="failedlogons"/>
    <property name="accountlocked"/>
    <property name="screenname"/>
    <property name="splashpage"/>
  </class>
</hibernate-mapping>