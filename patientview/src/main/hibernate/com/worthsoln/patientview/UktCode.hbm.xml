<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.uktransplant.UktCode" table="uktcode">
    <id name="id"
      unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="uktcode"/>
    <property name="description"/>
  </class>
</hibernate-mapping>