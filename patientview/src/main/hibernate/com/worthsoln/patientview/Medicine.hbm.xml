<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.medicine.Medicine" table="medicine">
    <id name="id"
        unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="nhsno"/>
    <property name="unitcode"/>
    <property name="startdate"/>
    <property name="name"/>
    <property name="dose"/>
  </class>
</hibernate-mapping>