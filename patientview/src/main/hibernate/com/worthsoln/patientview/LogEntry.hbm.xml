<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.logging.LogEntry" table="log">
    <id name="id"
      unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="date"/>
    <property name="nhsno"/>
    <property name="user"/>
    <property name="action"/>
    <property name="actor"/>
    <property name="unitcode"/>
    <property name="extrainfo"/>
  </class>
</hibernate-mapping>