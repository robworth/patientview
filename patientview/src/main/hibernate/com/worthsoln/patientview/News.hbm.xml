<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.news.News" table="news">
    <id name="id"
        unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="datestamp"/>
    <property name="unitcode"/>
    <property name="admin"/>
    <property name="patient"/>
    <property name="everyone"/>
    <property name="headline"/>
    <property name="body"/>
  </class>
</hibernate-mapping>