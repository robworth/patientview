<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.feedback.Feedback" table="feedback">
    <id name="id"
        unsaved-value="0">
      <generator class="increment"/>
    </id>
      <property name="username"/>
      <property name="name"/>
      <property name="nhsno"/>
      <property name="unitcode"/>
      <property name="datestamp"/>
      <property name="comment"/>
      <property name="commentedited"/>
      <property name="anonymous"/>
      <property name="makepublic"/>
  </class>
</hibernate-mapping>
