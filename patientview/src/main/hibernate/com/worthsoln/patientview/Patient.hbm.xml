<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.Patient" table="patient">
    <id name="nhsno">
      <generator class="assigned"/>
    </id>
    <property name="surname"/>
    <property name="forename"/>
    <property name="dateofbirth"/>
    <property name="sex"/>
    <property name="address1"/>
    <property name="address2"/>
    <property name="address3"/>
    <property name="postcode"/>
    <property name="telephone1"/>
    <property name="telephone2"/>
    <property name="mobile"/>
    <property name="centreCode"/>
    <property name="diagnosis"/>
    <property name="treatment"/>
    <property name="transplantstatus"/>
    <property name="hospitalnumber"/>
    <property name="gpname"/>
    <property name="gpaddress1"/>
    <property name="gpaddress2"/>
    <property name="gpaddress3"/>
    <property name="gppostcode"/>
    <property name="gptelephone"/>    
  </class>
</hibernate-mapping>