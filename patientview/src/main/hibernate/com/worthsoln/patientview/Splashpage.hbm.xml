<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.splashpage.SplashPage" table="splashpage">
    <id name="id"
        unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="name"/>
    <property name="live"/>
    <property name="headline"/>
    <property name="bodytext"/>
  </class>
</hibernate-mapping>