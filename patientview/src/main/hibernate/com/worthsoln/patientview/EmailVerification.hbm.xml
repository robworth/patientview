<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="com.worthsoln.patientview.user.EmailVerification" table="emailverification">
    <id name="id"
        unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="username"/>
    <property name="email"/>
    <property name="verificationcode"/>
    <property name="expirydatestamp"/>
  </class>
</hibernate-mapping>