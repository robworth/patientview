  <!-- ========== Action Mapping Definitions ============================== -->
  <action-mappings>
    <action path="/index"
            type="com.worthsoln.patientview.IndexAction" parameter="index">
      <forward name="success" path="/index.jsp"/>
    </action>
    <action path="/login"
            type="com.worthsoln.patientview.IndexAction" parameter="index">
      <forward name="success" path="/login.jsp" redirect="true"/>
    </action>
    <action path="/login-error"
            type="com.worthsoln.patientview.logon.LogonErrorAction">
      <forward name="success" path="/login-error.jsp"/>
    </action>
    <action path="/logged_in"
            type="com.worthsoln.patientview.logon.LoggedInAction" parameter="index">
      <forward name="patient" path="/login-success.jsp"/>
      <forward name="admin" path="/control/index.jsp"/>
    </action>
    <action path="/logout"
            type="com.worthsoln.patientview.logon.LogoutAction" parameter="index">
      <forward name="success" path="/index.jsp"/>
    </action>
    <action path="/forgotten-password"
      type="com.worthsoln.patientview.ForgottenPasswordAction" parameter="index">
      <forward name="input" path="/forgotten-password.jsp"/>
      <forward name="success" path="/forgotten-password-confirmation.jsp"/>
    </action>

    <action path="/setscreenname"
      type="com.worthsoln.forum.ScreenNameAction" parameter="index">
      <forward name="input" redirect="true" path="/set-screenname.jsp"/>
      <forward name="success" redirect="true" path="/forums/list.page"/>
    </action>


    <action path="/emailverification"
